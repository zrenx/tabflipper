{
    "name": "Tab Flipper ",
    "version": "1.0",
    "description": "Flipping the tabs for you. Interval and directions are optional.",
    "icons": {
	"48":"active.png"
    },
    "background": {"scripts":["background.js"]},
    "browser_action":{
	"default_icon": "inactive.png",
	"default_title": "try the tabs"
    },
    "permissions":["tabs", "http://*/*", "https://*/*"],
    "options_page": "options.html",
    "manifest_version": 1
}
